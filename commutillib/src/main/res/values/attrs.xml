<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ViewClipHelperStrategy">
        <!--  圆角大小   -->
        <attr name="clip_radius" format="dimension" />
        <attr name="clip_side" format="enum">
            <enum name="all" value="0" />
            <enum name="left" value="1" /> <!-- 左边 -->
            <enum name="top" value="2" /> <!-- 上边 -->
            <enum name="right" value="3" /> <!-- 右边 -->
            <enum name="bottom" value="4" /> <!-- 下边 -->
        </attr>
    </declare-styleable>
</resources>